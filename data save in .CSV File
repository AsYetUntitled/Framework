import pandas_datareader as web
import csv

import datetime as dt
  
 
#dt.datetime is a data type within the datetime module

start =      dt.datetime(2015, 9, 1)
end   =      dt.datetime(2017, 12, 31)
 
#DataReader method name is case sensitive

compny_name = ["GOOG","FB"]
#["GOOG","FB"]  are ticker's symbol of  Google , Facebook
data =        web.DataReader("compny_name", 'morningstar', start, end)
 
#invoke to_csv for df dataframe object form 

#DataReader method in the pandas_datareader library
 
#be open in another app, such as Excel
 
data.to_csv('data_file.csv') # data_file is a file name 
